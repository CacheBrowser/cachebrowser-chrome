{
  "name": "__MSG_appName__",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "icons": {
    "16": "images/icon.png",
    "128": "images/icon.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "background",
    "webRequest",
    "*://*.google.com/",
    "webRequestBlocking",
    "tabs",
    "proxy"
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "page_action": {
    "default_icon": {
      "19": "images/icon.png",
      "38": "images/icon.png"
    },
    "default_title": "CacheBrowser"
  }
}
